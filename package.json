{
  "name": "modern-node-be",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "nodemon": "MODE=application nodemon app.mjs",
    "docker:dev:up": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.dev.yml up",
    "docker:dev:down": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.dev.yml down",
    "docker:up": "docker-compose -f docker/docker-compose.yml up",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "start": "MODE=application NODE_ENV=production node app.mjs",
    "test:lint": "eslint lib tests app.mjs --ext mjs",
    "test:ava": "ava",
    "test:coverage": "c8 npm run test:ava",
    "test:audit": "npm audit",
    "test": "npm run test:lint && npm run test:audit && npm run test:coverage",
    "create:admin": "node ./bin/add_admin.mjs",
    "migration:db": "sequelize db:migrate --config lib/config.cjs --env db",
    "migration:test": "sequelize db:migrate --config lib/config.cjs --env test-db",
    "generate": "node --experimental-json-modules ./bin/generator.mjs"
  },
  "pre-commit": [
    "test:lint"
  ],
  "dependencies": {
    "@fastify/formbody": "^7.0.1",
    "@fastify/middie": "^8.0.0",
    "@fastify/multipart": "^7.1.0",
    "aws-sdk": "^2.685.0",
    "bluebird": "^3.7.2",
    "chista": "^1.0.0",
    "cls-hooked": "^4.2.2",
    "confme": "^1.0.0",
    "cors": "^2.8.5",
    "docopt": "^0.6.2",
    "fast-jwt": "^1.7.0",
    "fastify": "^4.3.0",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.7.6",
    "json-pointer": "^0.6.0",
    "livr": "^2.3.1",
    "livr-extra-rules": "^1.1.0",
    "mole-rpc": "^1.0.4",
    "mole-rpc-transport-ws": "^0.4.2",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.6",
    "nodemailer-sendmail-transport": "^1.0.2",
    "nodemailer-smtp-transport": "^2.7.4",
    "pino": "^6.3.0",
    "rename-keys": "^2.0.1",
    "sequelize": "^5.21.11",
    "sequelize-cli": "^5.5.1",
    "uuid": "^7.0.3",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "ava": "^3.8.2",
    "babel-eslint": "^10.1.0",
    "c8": "^7.1.2",
    "eslint": "^7.1.0",
    "eslint-config-webbylab": "^5.3.0",
    "eslint-plugin-ava": "^10.3.0",
    "eslint-plugin-fetch": "0.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-more": "0.3.2",
    "eslint-plugin-no-require-lodash": "^1.1.0",
    "eslint-plugin-prefer-spread": "^1.0.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^4.1.0",
    "line-reader": "^0.4.0",
    "node-fetch": "^2.6.0",
    "nodemailer-mock": "^1.5.1",
    "nodemailer-stub-transport": "^1.1.0",
    "nodemon": "^2.0.4",
    "pino-pretty": "^4.0.0",
    "pre-commit": "^1.2.2"
  },
  "description": "NodeJS Backend Starterkit",
  "main": "app.mjs",
  "repository": {
    "type": "git",
    "url": "http://gitlab.webbylab.com/boilerplates/modern-node-be"
  },
  "author": "WebbyLab"
}
